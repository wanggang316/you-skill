{
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:app:allow-set-app-theme",
    "core:window:allow-set-theme",
    "dialog:default",
    "dialog:allow-confirm",
    "fs:default",
    "fs:allow-applocaldata-write-recursive",
    "fs:allow-read-text-file",
    "shell:allow-open",
    "updater:default"
  ]
}
